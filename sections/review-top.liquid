<section class="review-banner" style="background-image: url('{{section.settings.img | img_url : 'master'}}');">
        <div class="container">
            <div class="row">
                <div class="col-md-8 m-auto">
                    <div class="contct-banner-content">
                        <h2>{{section.settings.text1}}</h2>
                        <p>{{section.settings.text2}}</p>
                    </div>
                </div>
                <div class="col-md-12">
                	 <div class="tabing">
                	    <ul class="nav nav-tabs" id="myTab" role="tablist">
							  <li class="nav-item rightim wow fadeInLeft" data-wow-duration="2s">
							    <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true"><img src="{{'bann-top.png' | asset_url}}" class="new-ban"></a>
							  </li>
							  <li class="nav-item rightim2 wow fadeInRight" data-wow-duration="2s">
							    <a class="nav-link" id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="false"><img src="{{'bann-new.png' | asset_url}}" class="new-ban"></a>
							  </li>
						</ul>
                    </div>
                </div>
               
            </div>
        </div>
    </section>

{% schema %}
  {
    "name": "Review-top",
    "settings": [
{
"id": "img",
"type": "image_picker",
"label": "Add Image"
},
{
"id": "text1",
"type": "textarea",
"label": "Add Heading"
},
{
"id": "text2",
"type": "textarea",
"label": "Add Paragraph"
}]
  }
{% endschema %}

