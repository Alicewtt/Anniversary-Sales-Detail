 {{'meazor.css' | asset_url | stylesheet_tag }}
  {{'meazor-responsive.css' | asset_url | stylesheet_tag }}






<section class="mezor__product-imgs">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-md-6">
                <div class="mezor__product-singleimg"> 
                
                  <div class="mezor__product-titlecnts mobile__new-design-g" style="display:none;"> 
                        <img src="{{ section.settings.img3 | img_url : 'master' }}">     
                        <h2>{{ section.settings.text1}}</h2> 
                        <p>{{ section.settings.text2}}</p>
                      </div>  
                    <div class="mezor__product-3dpremium"> 
                        <!-- <img src="assets/img/meazor-3dfirst.png"> -->
                       <!-- <img src="{{ section.settings.img1 | img_url : 'master' }}"> -->
                       <img src="{{ product.featured_media | img_url : 'master' }}">
                    </div> 
                    <div class="mezor__product-3dmezor"> 
                        <!-- <img src="assets/img/meazor-3d-img.png"> -->
                        <!--<img src="{{ section.settings.img2 | img_url : 'master' }}">-->
                        
                      <video class="mazor--video" id="videoclip" width="100%" height="100%" autoplay loop playshine muted>
                        <source id="mp4video" src="https://cdn.shopify.com/videos/c/o/v/6041c1b814e54dcfa269fd604c183afc.mp4" type="video/mp4" />
                      </video>

                    </div>     
                </div>   
            </div> 
            <div class="col-md-6">
                <div class="mezor__product-singlecnts"> 
                    <div class="mezor__product-titlecnts small_img-rvd" id="myDivbb"> 
                        <img src="{{ section.settings.img3 | img_url : 'master' }}">     
                        <h2>{{ section.settings.text1}}</h2> 
                        <p>{{ section.settings.text2}}</p>
                       
                        <div class="mazor__3d-price"><span style="font-size:22px" >Start from</span> {{ section.settings.price}}</div>
                       
                        <button type="button" {% unless product.available %} disabled {% endunless %}  price="{{ product.price | money_without_currency }}" class="deproduct select" id="{{ product.variants.first.id }}">{% if product.available %}BUY NOW {% else %} SOLD OUT {% endif %}</button>
                    </div> 
                    <div class="mezor__product-pluginout"> 
                        <div class="mezor__prd-back"><img src="{{ 'black__left.svg' | asset_url}}"> Back</div>
                        <ul>
                         <select name="id" id="{{ product.handle }}" style="display: none;">
                            {% for variant in product.variants %}
                              {% if variant.available %}
                              <option value="{{ variant.id }}">{{ variant.title }} - {{ variant.price | money }}</option>     
                              {% else %}
                              <option disabled="disabled">{{ variant.title }} - Sold Out</option>
                              {% endif %}
                            {% endfor %}
                              </select>  
                      

                        {% for variant in product.variants %}
                              
                            <li variants="{{ product.variants.size }}" number="{{ forloop.index }}" {% if product.variants.size == 1 %}style="pointer-events: none" {% endif %}class="variant_meazor_li premiummajoq__3dclick {%  if forloop.index < 2 %} mezor__activepack{% endif %}" price="{{ variant.price | money_without_currency }}" id="{{ variant.id }}" videourl="{{ variant.metafields.custom.videourl }}">
                               <div>
                                   <p >{% if product.variants.size == 1  and variant.title == 'Default Title' %}{{ product.title }} {% else %}{{ variant.title }} {% endif %}</p>
                                   {{ variant.metafields.custom.sub_text }}
                                   <p class="new_price">{{ variant.price | money }}</p>
                                   {{ variant.metafields.custom.sub_text }}
                               </div>
                                 <img class="tickIcon" src="{{ 'tick_Icon.png' | asset_img_url: 'master' }}">
                                 <div class="special-variant-specification   {%  if forloop.index < 2 %} activevari  {% endif %}">{{ variant.metafields.custom.variant_specification }}</div>
                           
                            </li>
                          {% endfor %}
                          
                          <h3 data-h="234">Top Pick Accessories</h3>
                              <div class="tab">
                            <button class="tablinks active" onclick="openCity(event, 'first')">Creative Classic</button>
                            <button class="tablinks" onclick="openCity(event, 'last')">Architech Advance</button>

                            </div>
                             <div id="first" class="tabcontent active"  style="display:block">
                             <ul class="new-list">
                          {%  for block in section.blocks %}
                         
                            {% if block.type == 'first' %}
                               {% assign pr1 = all_products[block.settings.pr1] %}
                              
                                <li class="premiummajoq__3dclick2"  price="{{ pr1.price | money_without_currency }}" id="{{ pr1.variants.first.id }}">
                                    <div>
                                      <img src="{{ pr1.featured_media | img_url : 'master'}}">
                                    </div>
                                    <div>
                                      <p><a href="{{ pr1.url }}">{{ pr1.title }}</a></p>
                                       <div class="tab-price-main">
                                        <div>
                                          <p class="tab-price">{{ pr1.price | money }}  {% if pr1.compare_at_price_max > pr1.price %}  <span class="special-pr">{{  pr1.compare_at_price_max | money }}</span> {%  endif %}</p>
                                        </div>
                                        <div>
                                        {% if pr1.compare_at_price_max > pr1.price %}  <span><strong> {{ pr1.compare_at_price_max | minus: pr1.price | times: 100.0 | divided_by: pr1.compare_at_price_max | money_without_currency | times: 100 | remove: '.0'}}% OFF </strong></span>{% endif %}
                                        </div>
                                      </div>
                                    </div>
                                    <span class="multiTick">
                                      <img class="tickIcon" src="{{ 'tick_Icon.png' | asset_img_url: 'master' }}">
                                    </span>
                                </li>
                               
                              {% endif %}
                            
                             {%  endfor %}
                             </ul>
                              </div>
                        
                            <div id="last" class="tabcontent">
                              <ul class="new-list">
                                 {%  for block in section.blocks %}
                               {% if block.type == 'last' %}
                               {% assign pr1 = all_products[block.settings.pr1] %}
                            <li class="premiummajoq__3dclick2"  price="{{ pr1.price | money_without_currency }}" id="{{ pr1.variants.first.id }}">
                              <div> 
                              <img src="{{ pr1.featured_media | img_url : 'master'}}" class="gggggg">
                              </div>
                              <div>
                                <p class="poto"><a href="{{ pr1.url }}">{{ pr1.title }}</a></p>
                                 <div class="tab-price-main">
                                        <div>
                                          <p class="tab-price">{{ pr1.price | money }}  {% if pr1.compare_at_price_max > pr1.price %}  <span class="special-pr">{{  pr1.compare_at_price_max | money }}</span> {%  endif %}</p>
                                        </div>
                                        <div>


                              
                                        {% if pr1.compare_at_price_max > pr1.price %}  <span><strong> {{ pr1.compare_at_price_max | minus: pr1.price | times: 100.0 | divided_by: pr1.compare_at_price_max | money_without_currency | times: 100 | remove: '.0'}}% OFF </strong></span>{% endif %}
                                        </div>
                                         
                                      </div>
                              </div>
                              <span class="multiTick">
                                      <img class="tickIcon" src="{{ 'tick_Icon.png' | asset_img_url: 'master' }}">
                                    </span>
                            </li>
                            
                              {% endif %}
                            
                          {% endfor %}
                              </ul>
                            </div>
                          <div class="btm-professional-s">  
                           <p>Total <span class="totalprice">{{ product.price | money}}</span></p>
                          <div class="btm-professional-btns"> 
                            <button type="button" class="finaladd mxform">Add to cart</button>
                            <button type="button" class="finaladdnow">Buy Now</button>
                           </div>
                          </div>
                        </ul>
                    </div>                  
                </div>  
            </div> 
        </div>     
    </div>  
</section>

 
