{% schema %}
  {
"name": "Home-banner-slider",
   "settings": [
     {
        "id": "enableCustomBanner",
        "type": "checkbox",
        "label": "Custom Banner"
      },
     {
       "id": "section_bg",
        "type": "color",
        "label": "section background"
       
     }
 ],
    "blocks": [
      {
        "type": "text",
        "name": "slider",
        "settings": [

         {
            "id": "image",
            "type": "image_picker",
            "label": "Add Image"
          },
          {
            "id": "mobile_image",
            "type": "image_picker",
            "label": "Add mobile Image"
          },
          {
            "id": "label_image",
            "type": "image_picker",
            "label": "Add label Image"
          },
          {
            "id": "text1",
            "type": "textarea",
            "label": "Add Heading"
          },
          {
            "id": "text2",
            "type": "textarea",
            "label": "Add button text"
          },
          {
            "id": "url2",
            "type": "url",
            "label": "Add Url"
          }
         
        ]
      }
    ],
"presets":[{
 "name": "home-banner-slider",
"category": "Custom Content"

}]
  }
{% endschema %}

{% stylesheet %}
{% endstylesheet %}

