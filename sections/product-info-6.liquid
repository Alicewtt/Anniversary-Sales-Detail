
<section class="hozo_connectmedia">
    <div class="container">
        <h2>{{section.settings.text1}}</h2>
        <div class="hozo__blue-img"><img src="{{section.settings.img1 | img_url : 'master'}}"></div>

        <div class="cork__mobile-connectmedia-inner">
            <ul class="nav nav-tabs" id="myTab" role="tablist">
              <li class="nav-item">
                <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true">
                    <span></span>
                     <img src="{{section.settings.img2 | img_url : 'master'}}">
                </a>
              </li>
              <li class="nav-item">
                <a class="nav-link" id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="false">
                    <span></span>
                     <img src="{{section.settings.img3 | img_url : 'master'}}">
                </a>
              </li>
              <li class="nav-item">
                <a class="nav-link" id="contact-tab" data-toggle="tab" href="#contact" role="tab" aria-controls="contact" aria-selected="false">
                    <span></span>
                   <img src="{{section.settings.img4 | img_url : 'master'}}">
                </a>
              </li>
            </ul>
            <div class="tab-content" id="myTabContent">
              <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
                  <div class="cork__tab-mobiles"><img src="{{section.settings.img5 | img_url : 'master'}}"></div>
              </div>
              <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
                  <div class="cork__tab-mobiles"><img src="{{section.settings.img6 | img_url : 'master'}}"></div>
              </div>
              <div class="tab-pane fade" id="contact" role="tabpanel" aria-labelledby="contact-tab">
                  <div class="cork__tab-mobiles"><img src="{{section.settings.img7 | img_url : 'master'}}"></div>  
              </div>
            </div>        
        </div>

        <div class="hozo_connectmedia-inner">
            <div class="row">
                <div class="col-md-4">
                    <div class="hozo__mediabox">
                        <span></span>  
                        <div class="hozo__boxbg-img" style="background:url('{{section.settings.img8 | img_url : 'master'}}')">
                            <div class="cork__img-pst">   
                                <img src="{{section.settings.img2 | img_url : 'master'}}">
                            </div> 
                        </div>          
                    </div>    
                </div>
                <div class="col-md-4">
                    <div class="hozo__mediabox">
                        <span></span>  
                        <div class="hozo__boxbg-img" style="background:url('{{section.settings.img9 | img_url : 'master'}}')">
                            <div class="cork__img-pst">   
                                <img src="{{section.settings.img3 | img_url : 'master'}}">
                            </div> 
                        </div>          
                    </div>    
                </div>
                <div class="col-md-4">
                    <div class="hozo__mediabox">
                        <span></span>  
                        <div class="hozo__boxbg-img" style="background:url('{{section.settings.img10 | img_url : 'master'}}')">
                            <div class="cork__img-pst">   
                                <img src="{{section.settings.img4 | img_url : 'master'}}">
                            </div> 
                        </div>          
                    </div>    
                </div>                
            </div>    
        </div>
    </div>
</section>





