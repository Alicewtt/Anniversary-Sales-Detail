    "meazor-faq-section-Rollova": {
      "type": "meazor-faq-section-Rollova",
      "blocks": {
        "09be2f56-1cea-437a-be65-e79aaa4c0866": {
          "type": "FAQ",
          "settings": {
            "qustion": "What is the measurement range and accuracy of NeoRuler?",
            "answer": "The measurement range of NeoRuler is 30.5cm (12 inches) and the measurement accuracy is 0.1mm (±0.004inch)"
          }
        },
        "28405080-159c-48d8-b0f0-3293fc325dc9": {
          "type": "FAQ",
          "settings": {
            "qustion": "How many scales are supported by NeoRuler?",
            "answer": "93 built-in scales are supported for maximum versatility, With NeoRuler’s customizable scale function, you can define an unlimited number of scales."
          }
        },
        "b6f92599-f14f-4d37-89ac-f5347e2fa0e4": {
          "type": "FAQ",
          "settings": {
            "qustion": "Which kinds of units are supported by NeoRuler?",
            "answer": "NeoRuler supports both Imperial(in decimal and fraction) and Metric units."
          }
        },
        "ece5386c-84c1-4f7e-b0c1-1fb8c11ad59d": {
          "type": "FAQ",
          "settings": {
            "qustion": "How long does the battery last per full charge?",
            "answer": "The theoretical standby time for a fully charged NeoRuler is around 45 days, and the continuous working time lasts around 12 hours."
          }
        },
        "bd489751-4eea-477a-945d-83eba5558138": {
          "type": "FAQ",
          "settings": {
            "qustion": "What is the battery capacity? Is the battery replaceable?",
            "answer": "The battery capacity is a rechargeable Li-on 1000 mAh battery. It can be used for about 1-2 weeks on a single charge. Like most electronic devices, the battery is designed to retain an estimated 80% of its original capacity at 500-800 complete charge cycles and is not replaceable."
          }
        },
        "3f961915-e62e-4539-9a17-38f4f907c235": {
          "type": "FAQ",
          "settings": {
            "qustion": "What languages does the MEAZOR app come with?",
            "answer": "The MEAZOR app supports 7 languages: French, Spanish, Chinese (Simplified), Chinese (Traditional), Japanese, Italian, German, and English (US)."
          }
        },
        "803e999f-dc25-460d-a223-37e3ad5a0189": {
          "type": "FAQ",
          "settings": {
            "qustion": "What file formats does the MEAZOR app support?",
            "answer": "Drawing Mode: export files in .dxf(2D) .dxf(3D) , Also export scale drawing in .pdf .jpg formats(auto-scale fit to A3(Metrics) \/ Tabloid(Imperial) size paper) List Mode: export files in excel sheet(.xls), .txt"
          }
        }
      },
