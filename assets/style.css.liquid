}

.navigation-landscape .list-col-2 {
  width: 50%;
}

.navigation-landscape .list-col-3 {
  width: 33%;
}

.navigation-landscape .list-col-4 {
  width: 25%;
}

.navigation-landscape .list-col-5 {
  width: 20%;
}
ul.megamenu-list.list-col-3 li {
    text-align: center;
}
.nav-menu > li > a {
  color: #343a40;
