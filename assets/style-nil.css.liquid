    font-weight: 600;
    font-size: 40px;
    text-transform: uppercase;
    line-height: 45px;
}



.mobile-foot .card-header {
  background-color: transparent;
  border: 0;
  margin: 0;
  padding: 0px 0;
  position: relative;
}
.mobile-foot .card {
  border: 0;
  border-bottom:0.5px solid rgba(0, 0, 0, 0.8); 
  border-radius:0!important;
}
.mobile-foot .card-header a {
  display: block;
  background-color: transparent;
  font-weight: 700;
  font-size: 20px;
  line-height: 42px;
  text-transform: capitalize;
  color: #000;
  text-align: left;
  padding: 0;
}
.mobile-foot .card ul {
  margin: 0;
  padding: 0;
}
.mobile-foot .card-body .footer-inner {
  padding: 0;
}
.mobile-foot .card-body {
  padding: 0;
}

.mobile-foot .card-header a:hover {
  background: transparent;
  color: black;
}
.mobile-foot .card-header a:focus {
  box-shadow: 0 0 0 transparent !important;
}

.mobile-foot .card-header .collapsed {
  position:relative;
}
.mobile-foot .card-header a:not(.collapsed):after {
  position: absolute;
  content: '\f00d';
  top: 0px;
  right: 15px;
  font-size: 14px;
  color: rgba(0, 0, 0, 0.8);
  font-family: 'FontAwesome';
}


.mobile-foot .card-header .collapsed:after {
  position:absolute;
  content:'\2b';
  top: 0px;
  right: 15px;
  font-size: 20px;
  font-weight: 900;
  color: rgba(0, 0, 0, 0.8);
}


.mobile-foot {
  display:none;
}

@media only screen and (max-width: 400px) {

.new-feature .Product-icon img {
width: 206px;
height: 200px;
}
  .easy-inner-image img {
width: 128px;
height: 127px;
}
}